{
  "extends": ["config:recommended"],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": ["/.*/"],
      "matchStrings": [
        "\\s*# amiFilter=(?<packageName>.*?)\\n[\\s\\S]*?id:\\s*(?<currentValue>ami-[a-z0-9]{17})"
      ],
      "depNameTemplate": "{{packageName}}",
      "datasourceTemplate": "aws-machine-image",
      "versioningTemplate": "aws-machine-image"
    }
  ]
}
